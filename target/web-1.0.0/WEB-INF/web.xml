<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



    <welcome-file-list>
        <welcome-file>start</welcome-file>
    </welcome-file-list>



    <!--HelloServlet configurations-->

    <!--Haytararum enq servlet u talis en anun u Class-->
    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>am.basic.jdbc.aold.HelloServlet</servlet-class>
    </servlet>

    <!--Ogtgorcelov haytararac servleti anun@ talis enq url -->
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/test-url</url-pattern>
    </servlet-mapping>


    <!--TestServlet configurations-->
    <!--Haytararum enq servlet u talis en anun u Class-->
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>am.basic.jdbc.aold.TestServlet</servlet-class>
    </servlet>

    <!--Ogtgorcelov haytararac servleti anun@ talis enq url -->
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/Edo</url-pattern>
    </servlet-mapping>



</web-app>
